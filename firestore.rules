rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /enquiries/{docId} {
      allow read, create: if request.auth != null
      allow delete, update: if request.auth.uid == resource.data.userId
    }
    match /userDetails/{docId} {
      allow read, create: if request.auth != null
      allow delete, update: if request.auth.uid == resource.data.userId
    }
  }
}